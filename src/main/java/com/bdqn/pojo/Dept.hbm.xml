<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.bdqn.pojo.Dept" table="dept" schema="test">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="dname">
            <column name="dname" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="des">
            <column name="des" sql-type="varchar(255)"/>
        </property>
    </class>
</hibernate-mapping>